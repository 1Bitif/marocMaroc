{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "rating": {
      "required": true,
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "SPRING ESSENTIALS",
        "CACTES",
        "BODY CARE",
        "HAIR CARE",
        "FACE",
        "GIFT IDEAS"
      ],
      "required": true
    }
  }
}
